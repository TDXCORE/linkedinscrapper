{
  "actorSpecification": 1,
  "name": "linkedin-profile-scraper-pro",
  "title": "LinkedIn Profile Scraper Pro with AI Analysis",
  "description": "Advanced LinkedIn profile scraper that extracts follower metrics, post analytics, and media URLs with AI-powered content analysis using Gemini Pro 2.5 and JigsawStack proxy rotation.",
  "version": "1.0.0",
  "buildTag": "latest",
  "dockerfile": "./Dockerfile",
  "input": "./input_schema.json",
  "storages": {
    "dataset": {
      "actorSpecification": 1,
      "title": "LinkedIn Profile Data",
      "views": {
        "overview": {
          "title": "Overview",
          "transformation": {
            "fields": [
              "profileUrl",
              "profile.name", 
              "profile.headline",
              "profile.followersCount",
              "profile.connectionsCount",
              "postsMetrics.totalPosts",
              "postsMetrics.avgReactions",
              "success"
            ]
          }
        }
      }
    }
  },
  "env": {},
  "defaultRunOptions": {
    "build": "latest",
    "timeoutSecs": 3600,
    "memoryMbytes": 4096
  }
}
